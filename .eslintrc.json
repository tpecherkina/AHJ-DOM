{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },

  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  
  
  }
}